<%= render :partial => 'admin/shared/configuration_menu' %>

<div data-hook="admin_relation_type_new_form_header">
  <h1><%= t("new_relation_type") %></h1>

  <%= render "shared/error_messages", :target => @relation_type %>
</div>

<div data-hook="admin_relation_type_new_form" >
  <%= form_for(:relation_type, :url => collection_url) do |f| %>
    <%= render :partial => "form", :locals => { :f => f } %>

    <div data-hook="admin_relation_type_new_form_buttons">
      <%= render :partial => "admin/shared/new_resource_links" %>
    </div>
  <% end %>
</div>
